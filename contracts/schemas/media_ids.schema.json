{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "media_ids fixture",
  "type": "object",
  "required": [
    "contract_version",
    "suite",
    "case_id",
    "input",
    "expected"
  ],
  "properties": {
    "contract_version": {
      "type": "integer",
      "minimum": 1
    },
    "suite": {
      "const": "media_ids"
    },
    "case_id": {
      "type": "string",
      "minLength": 1
    },
    "input": {
      "type": "object",
      "required": [
        "raw"
      ],
      "properties": {
        "raw": {
          "type": "string",
          "minLength": 1
        }
      },
      "additionalProperties": true
    },
    "expected": {
      "type": "object",
      "required": [
        "normalized",
        "kind"
      ],
      "properties": {
        "normalized": {
          "type": "string",
          "minLength": 1
        },
        "kind": {
          "type": "string",
          "enum": [
            "movie",
            "show",
            "season",
            "episode",
            "person",
            "unknown"
          ]
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": false
}
